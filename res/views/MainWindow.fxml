<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="480.0"
            prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="views.DesktopBugView">
    <TabPane layoutX="-48.0" layoutY="-86.0" prefHeight="400.0" prefWidth="600.0" rotateGraphic="true" side="LEFT"
             tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
             AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Tab closable="false" text="Bugs">
            <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0"
                      minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"
                            GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.rowSpan="2"
                            GridPane.vgrow="ALWAYS">
                    <top>
                        <Label fx:id="lblBug" alignment="CENTER" contentDisplay="CENTER" text="Bug"
                               textAlignment="CENTER" BorderPane.alignment="CENTER">
                            <font>
                                <Font size="24.0"/>
                            </font>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <BorderPane.margin>
                                <Insets bottom="5.0" top="5.0"/>
                            </BorderPane.margin>
                        </Label>
                    </top>
                    <center>
                        <TextArea fx:id="tfBug" prefHeight="200.0" prefWidth="200.0" text="Описание бага"
                                  BorderPane.alignment="CENTER">
                            <font>
                                <Font size="14.0"/>
                            </font>
                            <BorderPane.margin>
                                <Insets/>
                            </BorderPane.margin>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </TextArea>
                    </center>
                    <bottom>
                        <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="471.0" spacing="15.0"
                              BorderPane.alignment="CENTER_RIGHT">
                            <padding>
                                <Insets right="10.0"/>
                            </padding>
                            <Button mnemonicParsing="false" text="Fix"/>
                            <Button mnemonicParsing="false" text="Edit"/>
                            <Button mnemonicParsing="false" text="Delete"/>
                        </HBox>
                    </bottom>
                </BorderPane>
                <TitledPane collapsible="false" maxHeight="1.7976931348623157E308" text="Bugs List"
                            GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.valignment="TOP"
                            GridPane.vgrow="ALWAYS">
                    <ListView fx:id="listBugs" layoutX="-53.0" layoutY="-19.0" prefWidth="100.0"
                              GridPane.columnIndex="0" GridPane.hgrow="NEVER" GridPane.rowIndex="0"
                              GridPane.vgrow="ALWAYS"/>
                </TitledPane>
                <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                        text="+" textAlignment="CENTER" wrapText="true" GridPane.halignment="CENTER"
                        GridPane.hgrow="SOMETIMES" GridPane.rowIndex="1" GridPane.vgrow="SOMETIMES"
                        onAction="#onBugAdd"/>
            </GridPane>
        </Tab>
        <Tab closable="false" text="Improvements">
            <GridPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <TitledPane collapsible="false" maxHeight="1.7976931348623157E308" prefWidth="100.0"
                            text="Improvements" GridPane.hgrow="NEVER" GridPane.valignment="CENTER"
                            GridPane.vgrow="ALWAYS">
                    <ListView fx:id="listImprovements" layoutX="-53.0" layoutY="-19.0" prefWidth="100.0"
                              GridPane.columnIndex="0" GridPane.hgrow="NEVER" GridPane.rowIndex="0"
                              GridPane.vgrow="ALWAYS"/>
                </TitledPane>
                <Button alignment="CENTER" contentDisplay="CENTER"
                        mnemonicParsing="false" text="+" textAlignment="CENTER" wrapText="true"
                        GridPane.halignment="CENTER" GridPane.hgrow="SOMETIMES" GridPane.rowIndex="1"
                        GridPane.vgrow="SOMETIMES" onAction="#onImprovementAdd"/>
                <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"
                            GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.rowSpan="2"
                            GridPane.vgrow="ALWAYS">
                    <bottom>
                        <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="471.0" spacing="15.0"
                              BorderPane.alignment="CENTER_RIGHT">
                            <padding>
                                <Insets right="10.0"/>
                            </padding>
                            <Button mnemonicParsing="false" text="Fix"/>
                            <Button mnemonicParsing="false" text="Edit"/>
                            <Button mnemonicParsing="false" text="Delete"/>
                        </HBox>
                    </bottom>
                    <center>
                        <TextArea fx:id="tfImprovement" prefHeight="200.0" prefWidth="200.0"
                                  text="Описание улучшения" BorderPane.alignment="CENTER">
                            <font>
                                <Font size="14.0"/>
                            </font>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                            <BorderPane.margin>
                                <Insets/>
                            </BorderPane.margin>
                        </TextArea>
                    </center>
                    <top>
                        <Label fx:id="lblImprovement" alignment="CENTER" contentDisplay="CENTER"
                               text="Improvement" textAlignment="CENTER" BorderPane.alignment="CENTER">
                            <font>
                                <Font size="24.0"/>
                            </font>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <BorderPane.margin>
                                <Insets bottom="5.0" top="5.0"/>
                            </BorderPane.margin>
                        </Label>
                    </top>
                </BorderPane>
            </GridPane>
        </Tab>
        <Tab closable="false" text="Ideas">
            <GridPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <TitledPane collapsible="false" maxHeight="1.7976931348623157E308" text="Ideas List"
                            GridPane.hgrow="NEVER" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
                    <ListView fx:id="listIdeas" layoutX="-53.0" layoutY="-19.0" prefWidth="100.0"
                              GridPane.columnIndex="0" GridPane.hgrow="NEVER" GridPane.rowIndex="0"
                              GridPane.vgrow="ALWAYS"/>
                </TitledPane>
                <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"
                            GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.rowSpan="2"
                            GridPane.vgrow="ALWAYS">
                    <bottom>
                        <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="471.0" spacing="15.0"
                              BorderPane.alignment="CENTER_RIGHT">
                            <padding>
                                <Insets right="10.0"/>
                            </padding>
                            <Button mnemonicParsing="false" text="Fix"/>
                            <Button mnemonicParsing="false" text="Edit"/>
                            <Button mnemonicParsing="false" text="Delete"/>
                        </HBox>
                    </bottom>
                    <center>
                        <TextArea fx:id="tfIdea" prefHeight="200.0" prefWidth="200.0" text="Описание идеи"
                                  BorderPane.alignment="CENTER">
                            <font>
                                <Font size="14.0"/>
                            </font>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                            <BorderPane.margin>
                                <Insets/>
                            </BorderPane.margin>
                        </TextArea>
                    </center>
                    <top>
                        <Label fx:id="lblIdea" alignment="CENTER" contentDisplay="CENTER" text="Idea"
                               textAlignment="CENTER" BorderPane.alignment="CENTER">
                            <font>
                                <Font size="24.0"/>
                            </font>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <BorderPane.margin>
                                <Insets bottom="5.0" top="5.0"/>
                            </BorderPane.margin>
                        </Label>
                    </top>
                </BorderPane>
                <Button alignment="CENTER" contentDisplay="CENTER"
                        mnemonicParsing="false" text="+" textAlignment="CENTER" wrapText="true"
                        GridPane.halignment="CENTER" GridPane.hgrow="SOMETIMES" GridPane.rowIndex="1"
                        GridPane.vgrow="SOMETIMES" onAction="#onIdeaAdd"/>
            </GridPane>
        </Tab>
    </TabPane>
</AnchorPane>
